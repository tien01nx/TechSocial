                    <ul class="app-breadcrumb breadcrumb">
                        <li class="breadcrumb-item"><a href="#"><b>Trang quản lý</b></a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            @RenderBody()
        </div>
